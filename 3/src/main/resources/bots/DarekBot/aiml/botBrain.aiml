     <topic name="FIRMA">
        <category>
            <pattern>*</pattern>
            <template>
                <random>
                    <li>Chcialbys otrzymac kredyt na samochod?</li>
                    <li>Czy chcesz wizac samochod w leasing?</li>
                </random>
            </template>
        </category>

        <category>
            <pattern>Tak _</pattern>
            <template>Dobrze, przygotuje odpowiednia oferte.</template>
        </category>

        <category>
             <pattern>Nie _</pattern>
            <template><think><set name="topic">*</set></think>Dobrze, przejdzmy do czegos innego</template>
        </category>
     </topic>

     <category>
        <pattern>_ kredyt</pattern>
        <template><think><set name="topic">FIRMA</set></think>Dobrze porozmawiajmy o formach finansowania</template>
     </category>

